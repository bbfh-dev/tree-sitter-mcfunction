  "name": "mcfunction",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "_space": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "backslash"
        }
      ]
    },
    "backslash": {
      "type": "PATTERN",
      "value": "\\s*\\\\\\r?\\n\\s*"
    },
    "_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\s+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "comment"
                },
                {
                  "type": "SYMBOL",
                  "name": "command"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": "[^\\r\\n]*"
          }
        ]
      }
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "macro_indicator"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_execute_command"
            },
            {
              "type": "SYMBOL",
              "name": "_generic_command"
            }
          ]
        }
      ]
    },
    "_execute_command": {
      "type": "PREC_RIGHT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "execute"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_space"
                },
                {
                  "type": "SYMBOL",
                  "name": "_subcommand"
                }
              ]
            }
          },
          {
            "type": "SYMBOL",
            "name": "_space"
          },
          {
            "type": "STRING",
            "value": "run"
          },
          {
            "type": "SYMBOL",
            "name": "_space"
          },
          {
            "type": "SYMBOL",
            "name": "command"
          }
        ]
      }
    },
