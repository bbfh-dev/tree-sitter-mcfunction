{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mcfunction",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "backslash": {
      "type": "PATTERN",
      "value": "\\s*\\\\\\r?\\n\\s*"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "_space": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "backslash"
        }
      ]
    },
    "_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\s+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "mime_comment"
                },
                {
                  "type": "SYMBOL",
                  "name": "comment"
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "macro_indicator"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "execute_statement"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "command"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "macro_indicator": {
      "type": "PATTERN",
      "value": "\\$"
    },
    "macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$("
        },
        {
          "type": "PATTERN",
          "value": "[a-z_]+"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "substitution": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%["
        },
        {
          "type": "PATTERN",
          "value": "[a-z_]+"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
